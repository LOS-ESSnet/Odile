{
	"id": "/users/sl7cob/odil",
	"instances": 1,
	"cpus": 1,
	"mem": 4096,
	"disk": 0,
	"gpus": 0,
	"cmd": "gunicorn -w 4 -b :8050 app:server",
	"container": {
		"type": "DOCKER",
		"docker": {
			"image": "docker-registry.beta.innovation.insee.eu/sl7cob/odil",
			"network": "BRIDGE",
			"portMappings": [
				{
					"containerPort": 8050,
					"hostPort": 0,
					"protocol": "tcp",
					"name": "default"
				}
			],
			"privileged": false,
			"forcePullImage": true
		}
	},
	"healthChecks": [
		{
			"gracePeriodSeconds": 120,
			"intervalSeconds": 30,
			"timeoutSeconds": 5,
			"maxConsecutiveFailures": 3,
			"portIndex": 0,
			"protocol": "MESOS_TCP"
		}
	],
	"labels": {
		"HAPROXY_GROUP": "external",
		"HAPROXY_0_VHOST": "odil.alpha.innovation.insee.eu",
		"HAPROXY_0_REDIRECT_TO_HTTPS": "true",
		"HAPROXY_0_FRONTEND_HEAD": "",
		"HAPROXY_0_FRONTEND_BACKEND_GLUE": "",
		"ONYXIA_TITLE": "ODIL",
		"ONYXIA_LOGO": "https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcTQNgLtCzvdT7B5ASszMYHfkUF2Tik4o5LR0HEvet_rDdGlvwFD",
		"ONYXIA_SUBTITLE": "odil sous-titre",
      "ONYXIA_STATUS":"alpha",
      "ONYXIA_DESCRIPTION": "Prototype ODIL",
      "ONYXIA_URL": "https://odil.alpha.innovation.insee.eu",
      "ONYXIA_NAME": "ODIL",
      "ONYXIA_SCM":"https://git.stable.innovation.insee.eu/sl7cob/odil"
	},
	"requirePorts": false
}
